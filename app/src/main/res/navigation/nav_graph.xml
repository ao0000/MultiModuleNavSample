<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/AFragment">

    <fragment
        android:id="@+id/AFragment"
        android:name="me.ao0000.multimodulenavsample.AFragment"
        android:label="AFragment">
        <action
            android:id="@+id/action_AFragment_to_CFragment"
            app:destination="@id/CFragment" />
        <action
            android:id="@+id/action_AFragment_to_BFragment"
            app:destination="@id/BFragment" />
    </fragment>
    <fragment
        android:id="@+id/BFragment"
        android:name="me.ao0000.multimodulenavsample.BFragment"
        android:label="BFragment">
        <argument
            android:name="fromName"
            app:argType="string" />
    </fragment>

    <fragment
        android:id="@+id/CFragment"
        android:name="me.ao0000.multimodulenavsample.CFragment"
        android:label="CFragment">
        <argument
            android:name="fromName"
            app:argType="string" />
    </fragment>
</navigation>